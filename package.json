{
  "name": "vite-react-ts",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "all": "rm -rf node_modules && rm -rf yarn.lock && yarn install",
    "dev": "set NODE_ENV=development && vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,tsx ./src/",
    "lint:fix": "eslint --fix --ext .ts,tsx  ./src/",
    "prettier": "prettier --check \"src/**/(*.tsx|*.ts|*.jsx|*.js|*.scss|*.css)\"",
    "prettier:fix": "prettier --write \"src/**/(*.tsx|*.ts|*.jsx|*.js|*.scss|*.css)\"",
    "prepare": "husky install",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-regular-svg-icons": "^6.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "@hookform/error-message": "^2.0.0",
    "@hookform/resolvers": "^2.8.8",
    "@reduxjs/toolkit": "^1.8.0",
    "@types/nprogress": "^0.2.0",
    "antd": "^4.18.9",
    "axios": "^0.26.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "clsx": "^1.1.1",
    "dequal": "^2.0.2",
    "framer-motion": "^6.2.8",
    "fs": "^0.0.1-security",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "nprogress": "^0.2.0",
    "query-string": "^7.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.27.1",
    "react-lottie": "^1.2.3",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.2",
    "react-use": "^17.3.2",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/lodash": "^4.14.179",
    "@types/moment": "^2.13.0",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-lottie": "^1.2.6",
    "@types/react-redux": "^7.1.23",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux-logger": "^3.0.9",
    "@types/redux-persist": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "@typescript-eslint/typescript-estree": "^5.13.0",
    "@vitejs/plugin-react": "^1.0.7",
    "autoprefixer": "^10.4.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "less-vars-to-js": "^1.3.0",
    "prettier": "^2.5.1",
    "redux-logger": "^3.0.6",
    "sass": "^1.49.9",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vite-plugin-imp": "^2.1.6",
    "vite-plugin-inspect": "^0.4.3",
    "vite-plugin-optimize-persist": "^0.1.2",
    "vite-plugin-package-config": "^0.1.1",
    "vite-plugin-pages": "^0.21.4",
    "vite-plugin-pages-sitemap": "^1.1.11",
    "vite-plugin-windicss": "^1.8.3",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.5.9",
    "windicss": "^3.5.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 ie version"
    ]
  },
  "vite": {
    "optimizeDeps": {
      "include": [
        "@fortawesome/free-solid-svg-icons",
        "@fortawesome/react-fontawesome",
        "@hookform/error-message",
        "@hookform/resolvers/class-validator",
        "@reduxjs/toolkit",
        "antd",
        "antd/lib/input/TextArea",
        "class-validator",
        "clsx",
        "framer-motion",
        "lodash/cloneDeep",
        "react",
        "react-dom",
        "react-hook-form",
        "react-lottie",
        "react-redux",
        "react-router",
        "react-router-dom",
        "react-use",
        "redux-logger",
        "redux-persist",
        "redux-persist/integration/react",
        "redux-persist/lib/storage"
      ]
    }
  }
}
